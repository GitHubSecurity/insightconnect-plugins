#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_cef import connection, actions, triggers


Name = 'CEF'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'Work with Common Event Format (CEF) data'


class ICONCef(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.CreateString())

        self.add_action(actions.ParseMultiple())

        self.add_action(actions.ParseSingle())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONCef())
    cli.run()


if __name__ == "__main__":
    main()
