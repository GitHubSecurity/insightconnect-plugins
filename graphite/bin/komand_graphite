#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_graphite import connection, actions, triggers


Name = 'Graphite'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'Graphite is an enterprise-ready monitoring tool that allows you store and graph metrics'


class ICONGraphite(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.MetricsExpand())

        self.add_action(actions.MetricsFind())

        self.add_action(actions.MetricsIndex())

        self.add_action(actions.MetricsRender())

        self.add_action(actions.MetricsRetrieve())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONGraphite())
    cli.run()


if __name__ == "__main__":
    main()
