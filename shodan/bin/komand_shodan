import komand
from komand_shodan import connection, actions, triggers


Name = 'Shodan'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'A Search Engine for Internet-Connected Devices'


class ICONShodan(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.HostInformation())

        self.add_action(actions.ShodanQuery())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONShodan())
    cli.run()


