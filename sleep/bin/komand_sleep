import komand
from komand_sleep import connection, actions, triggers


Name = 'Sleep'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'Suspend execution for an interval of time'


class ICONSleep(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Sleep())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONSleep())
    cli.run()


