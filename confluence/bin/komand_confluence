#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_confluence import connection, actions, triggers


Name = 'Confluence'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'Confluence Wiki'


class ICONConfluence(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.GetPage())

        self.add_action(actions.GetPageContent())

        self.add_action(actions.StorePageContent())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONConfluence())
    cli.run()


if __name__ == "__main__":
    main()
