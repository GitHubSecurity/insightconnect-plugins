#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from komand_phabricator import connection, actions, triggers


Name = 'Phabricator'
Vendor = 'rapid7'
Version = '1.0.0'
Description = 'Phabricator is a suite of open source tools for peer code review, task management, and project communication'


class ICONPhabricator(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name,
                vendor=Vendor,
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.Assign())

        self.add_action(actions.Claim())

        self.add_action(actions.Close())

        self.add_action(actions.Createtask())

        self.add_action(actions.Priority())

        self.add_action(actions.Projects())

        self.add_action(actions.Status())

        self.add_action(actions.Subscribe())

        self.add_action(actions.Unsubscribe())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONPhabricator())
    cli.run()


if __name__ == "__main__":
    main()
